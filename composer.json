{
    "name": "dollyaswin/functional-php",
    "description": "Functional Programming in PHP",
    "type": "project",
    "license": "BSD-3-Clause",
    "keywords": ["php7",  "functional programming"],
    "homepage": "https://aqilix.com/",
    "config": {
        "process-timeout": 5000
    },
    "require": {
        "php": "^5.6 || ^7.0",
        "lstrojny/functional-php": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5",
        "squizlabs/php_codesniffer": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "FunctionalExample\\": "src/"
        }
    },
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "test": "phpunit"
    }
}
